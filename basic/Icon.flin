isNavigation = name.starts_with("menu") || name.starts_with("home") || name.starts_with("compass") || name.starts_with("map") || name.starts_with("navigation") || name.starts_with("route") || name.starts_with("signpost") || name.starts_with("milestone")

// Route to category component by prefix
<span class={"icon " ++ sizeClass} style={"color: " ++ color} role="img" aria-label={name}>
    {if isArrow}
        <ArrowIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isChart}
        <ChartIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isFile}
        <FileIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isMedia}
        <MediaIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isCommunication}
        <CommunicationIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isDevice}
        <DeviceIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isEditor}
        <EditorIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isShape}
        <ShapeIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isWeather}
        <WeatherIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isUser}
        <UserIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isSecurity}
        <SecurityIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isLayout}
        <LayoutIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else if isNavigation}
        <NavigationIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {else}
        <MiscIcons name={name} size={sizeValue} stroke={strokeWidth}/>
    {/if}
</span>

<style scoped>
.icon { display: inline-flex; align-items: center; justify-content: center; }
