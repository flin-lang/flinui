isDevice = name.starts_with("monitor") || name.starts_with("laptop") || name.starts_with("smartphone") || name.starts_with("tablet") || name.starts_with("tv") || name.starts_with("watch") || name.starts_with("printer") || name.starts_with("keyboard") || name.starts_with("mouse") || name.starts_with("cpu") || name.starts_with("hard-drive") || name.starts_with("server") || name.starts_with("wifi") || name.starts_with("bluetooth")
isEditor = name.starts_with("bold") || name.starts_with("italic") || name.starts_with("underline") || name.starts_with("strikethrough") || name.starts_with("list") || name.starts_with("align") || name.starts_with("indent") || name.starts_with("text") || name.starts_with("type") || name.starts_with("heading") || name.starts_with("quote") || name.starts_with("code") || name.starts_with("link") || name.starts_with("unlink")
isShape = name.starts_with("circle") || name.starts_with("square") || name.starts_with("rectangle") || name.starts_with("triangle") || name.starts_with("pentagon") || name.starts_with("hexagon") || name.starts_with("octagon") || name.starts_with("star") || name.starts_with("heart") || name.starts_with("diamond")
isWeather = name.starts_with("sun") || name.starts_with("moon") || name.starts_with("cloud") || name.starts_with("wind") || name.starts_with("rain") || name.starts_with("snow") || name.starts_with("thunder") || name.starts_with("fog") || name.starts_with("thermometer") || name.starts_with("droplet") || name.starts_with("umbrella")
isUser = name.starts_with("user") || name.starts_with("users") || name.starts_with("contact") || name.starts_with("person") || name.starts_with("accessibility")
isSecurity = name.starts_with("lock") || name.starts_with("unlock") || name.starts_with("key") || name.starts_with("shield") || name.starts_with("eye") || name.starts_with("eye-off") || name.starts_with("fingerprint") || name.starts_with("scan")
