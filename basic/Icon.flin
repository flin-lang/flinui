name = props.name || "circle"
size = props.size || "md"
color = props.color || "currentColor"
strokeWidth = props.strokeWidth || "2"

// Size mapping
sizeValue = match size {
    "xs" -> "12"
    "sm" -> "16"
    "lg" -> "24"
    "xl" -> "32"
    _ -> "20"
}

sizeClass = match size {
    "xs" -> "icon-xs"
    "sm" -> "icon-sm"
    "lg" -> "icon-lg"
    "xl" -> "icon-xl"
    _ -> "icon-md"
}

// Category detection (pre-computed outside view mode)
isArrow = name.starts_with("arrow") || name.starts_with("chevron") || name.starts_with("corner") || name.starts_with("move") || name.starts_with("expand") || name.starts_with("shrink") || name.starts_with("maximize") || name.starts_with("minimize")
